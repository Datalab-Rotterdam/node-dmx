{
  "name": "node-dmx",
  "version": "1.0.1",
  "description": "node-dmx is a TypeScript library for controlling DMX over IP from Node.js.",
  "main": "./dist/index.es.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "example/sacn-animate": "tsx examples/sacn-animate.ts",
    "example/sacn-comet-chase": "tsx examples/sacn-comet-chase.ts",
    "example/sacn-fire-flicker": "tsx examples/sacn-fire-flicker.ts",
    "example/sacn-police-strobe": "tsx examples/sacn-police-strobe.ts",
    "example/sacn-breathing-pastels": "tsx examples/sacn-breathing-pastels.ts",
    "example/sacn-color-wipe": "tsx examples/sacn-color-wipe.ts",
    "example/sacn-sparkle": "tsx examples/sacn-sparkle.ts",
    "example/sacn-theater-chase": "tsx examples/sacn-theater-chase.ts",
    "example/sacn-aurora": "tsx examples/sacn-aurora.ts",
    "example/sacn-pulse-stack": "tsx examples/sacn-pulse-stack.ts",
    "example/sacn-storm": "tsx examples/sacn-storm.ts",
    "example/sacn-halloween": "tsx examples/sacn-halloween.ts",
    "example/sacn-christmas": "tsx examples/sacn-christmas.ts",
    "example/sacn-rainbow": "tsx examples/sacn-rainbow.ts",
    "example/sacn-listen-matrix": "tsx examples/sacn-listen-matrix.ts",
    "example/artnet-listen-matrix": "tsx examples/artnet-listen-matrix.ts",
    "example/rdmnet-listen": "tsx examples/rdmnet-listen.ts",
    "example/rdmnet-discover": "tsx examples/rdmnet-discover.ts",
    "example/rdmnet-interop-smoke": "tsx examples/rdmnet-interop-smoke.ts",
    "example/fixture-rgbww5": "tsx examples/fixture-rgbww5.ts",
    "test:interop:rdmnet": "vitest run test/rdmnet-interop.integration.test.ts",
    "example/network-debugger": "tsx examples/network-debugger/app.ts",
    "example/sacn-basic": "tsx examples/sacn-basic.ts",
    "example/list-network-interfaces": "tsx examples/list-network-interfaces.ts",
    "docs:build": "typedoc --options typedoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataLab-Rotterdam/node-dmx.git"
  },
  "keywords": [
    "dmx",
    "dmx512",
    "dmx512-a",
    "lighting",
    "lighting-control",
    "stage-lighting",
    "typescript",
    "nodejs",
    "artnet",
    "art-net",
    "sACN",
    "sacn",
    "e1.31",
    "ACN",
    "rdmnet",
    "e1.33",
    "rdm",
    "e1.20",
    "enttec",
    "esta"
  ],
  "author": "A.P.A. Slaa",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/DataLab-Rotterdam/node-dmx/issues"
  },
  "homepage": "https://datalab-rotterdam.github.io/node-dmx/",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "tsx": "^4.20.4",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "node-dmx(release): ðŸš€ ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
